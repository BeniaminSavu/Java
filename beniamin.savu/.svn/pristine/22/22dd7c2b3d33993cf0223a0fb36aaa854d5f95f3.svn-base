package com.iquestgroup.remotelearning.week3.p1.beniamin;

public class FrenchPerson extends Person{
	private int birthDate;
	
	FrenchPerson(String fullName, int birthDate) {
		super(fullName);
		this.birthDate = birthDate;
	}

	@Override
	public int getBirthDate() {
		return birthDate;
	}

	@Override
	public void selfDescribe() {
		System.out.println("Hello from FrenchPerson I was born in " + this.getBirthDate());
	}

}
