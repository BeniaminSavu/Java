package com.iquestint.ju;

public interface List<T> {

	/**
	 * Adds an element to the list
	 * @param element element to add
	 */
	void add(T element);
	
	/**
	 * Return the element at that position
	 * @param position index to return
	 * @return the element on the specified position
	 */
	T get(int position);
	
	/**
	 * Checks if the element is in the list
	 * @param element element to be checked
	 * @return true if the element is in the list, false otherwise
	 */
	boolean contains(T element);
	
	/**
	 * Checks if another list is in the list
	 * @param foreignList list to be checked
	 * @return true if all the element from foreignList are contained, false otherwise
	 */
	boolean containsAll(List<T> foreignList);
	
	/**
	 * Get the size of the list
	 * @return the size of the list
	 */
	int size();
	
	/**
	 * Displays all the operations that had been performed on the list
	 */
	void displayOperations();
}
