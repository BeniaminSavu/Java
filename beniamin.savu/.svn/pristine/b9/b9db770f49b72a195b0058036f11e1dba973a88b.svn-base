package com.iquest.week7.pr1;

import java.util.Comparator;

/**
 * 
 * @author Beniamin
 *	Comparator for the Country Class that only cares about the capital
 */
public class CapitalComparator implements Comparator<Country> {

	/**
	 * Compares its two arguments. Returns a negative integer, zero, or a positive 
	 * integer as the first argument is less than, equal to, or greater than the second.
	 */
	public int compare(Country c1, Country c2) {
		return c1.getCapital().compareTo(c2.getCapital());
	}

}
